<div class="col-sm-6">

    <h2>Burgers to Eat!</h2>

    <ul>
        {{#each burgers}}
            {{#unless devoured}}
                {{> burgers/burger-block devoured=false}}
            {{/unless}}
        {{/each}}
    </ul>

</div>

<div class="col-sm-6">

    <h2>Eaten Burgers!</h2>

    {{#each burgers}}
        {{#if devoured}}
            {{> burgers/burger-block devoured=true}}
        {{/if}}
    {{/each}}

</div>